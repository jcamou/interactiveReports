          <input type="text" class="form-control" id="targeturl" name="targeturl" placeholder="Target Domain (thehoth.com)" value="{{ @targeturl }}">
        </div>
        <div class="form-group">
          <label>Select the query mode to use for this check</label>
          <select class="form-control" id="mode" name="mode">
            <option value="subdomains" {{ @mode == "subdomains" ? "selected" : "" }}>Domain with all it's subdomains (*.domain.com/*)</option>
            <option value="domain" {{ @mode == "domain" ? "selected" : "" }}>Domain without subdomains (domain.com/*)</option>
            <option value="prefix" {{ @mode == "prefix" ? "selected" : "" }}>URL Prefix (domain.com/path/*)</option>
